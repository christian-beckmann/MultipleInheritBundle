{
    "name": "franckranaivo/multiple-inherit-bundle",
    "type": "symfony-bundle",
    "description": "This Symfony3 bundle provides inheritance between multiple child bundles and one parent.",
    "keywords": ["symfony", "inheritance", "bundle"],
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Franck RANAIVO-HARISOA",
            "email": "franck@init.mg"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/framework-bundle": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": { "Init\\Bundle\\MultipleInheritBundle": "" }
    },
    "target-dir": "Init/Bundle/MultipleInheritBundle",
    "minimum-stability": "dev"
}
